# Backend API URL
# For local development: http://localhost:8000
# For Docker: http://backend:8000
VITE_BACKEND_URL=http://localhost:8000

# LLM Provider API Keys
# At least one of these must be set for the backend to work
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
DEFAULT_LLM_PROVIDER=openai                # or openai / anthropic / deepseek / openrouter

# Neo Blockchain RPC URL
NEO_RPC_URL=https://testnet1.neo.org:20332

# Optional: CORS Origins (comma-separated)
# CORS_ORIGINS=http://localhost:80,http://yourdomain.com

